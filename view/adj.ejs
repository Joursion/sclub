<% include header.ejs %>

<div class="container">
    <div class="row">
        <% include partials/admin.ejs %>
        <section class="admin">
            <div class="container">
                <div class="rows">
                    <div class="col-md-9 col-ms-9 admin_job">
                        <% jobs.forEach(function (job) { %>
                        <div class="job">
                            用户:  <a href="/u/<% job.name %>"><%= job.name %> </a>
                            注册时间:  <%= job.reg_date %>
                            <div class="dropdown option">
                                <button class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                    操作
                                    <span class="caret"></span>
                                </button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                    <li><a href="#">删除</a></li>
                                    <li><a href="#">编辑</a></li>
                                </ul>
                            </div>
                        </div>
                        <% }); %>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>

<% include good.ejs%>